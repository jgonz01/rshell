(echo a) || (echo b)
(echo a) && (echo b)
(echo a) ; (echo b)
(echo a && echo b) && echo c
echo a && (echo b && echo c)
(ps && ls) || (echo a && echo b && echo c)
(echo a && echo b && echo c) && (echo d && echo e)
(echo a || echo b || echo c) || (echo d || echo e)
(echo a || echo b || echo c) ; (echo d || echo e) 
(echo a || echo b || echo c) && (echo d || echo e) 
(echo a) && (echo b) && (echo c) 
